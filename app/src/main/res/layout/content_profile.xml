<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbarStyle="insideOverlay"
    android:background="@color/zalivon"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.httpso_hello.hello.activity.ProfileActivity"
    tools:showIn="@layout/app_bar_profile">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/profile_content"
        android:background="@color/zalivon"
        android:scrollbars="vertical"
        android:paddingBottom="4dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/main_white_color_hello"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/photo_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:text="Нет фото"
                    android:textColor="@color/main_blue_color_hello" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:id="@+id/new_photo_button"
                    android:orientation="horizontal"
                    android:gravity="right"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_gravity="center"
                        android:background="@mipmap/ic_action_photo"
                        android:backgroundTint="@color/main_dark_grey_color_hello"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:text="Новое фото"
                        android:gravity="center"/>

                </LinearLayout>

            </LinearLayout>

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="6dp"
                android:scrollbars="none">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/linearLayoutForPhoto"
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <GridView
                            android:id="@+id/profile_photos_list"
                            android:layout_width="wrap_content"
                            android:layout_height="120dp"
                            android:paddingLeft="10dp"
                            android:columnWidth="120dp"
                            android:gravity="center"
                            android:horizontalSpacing="4dp"
                            android:scrollbarAlwaysDrawHorizontalTrack="true"
                            android:scrollbarAlwaysDrawVerticalTrack="true"
                            android:scrollbars="horizontal"
                            android:stretchMode="none"
                            android:verticalSpacing="1dp"/>
                    </LinearLayout>

                </FrameLayout>

            </HorizontalScrollView>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/new_view"/>

        </LinearLayout>

        <RelativeLayout
            android:id="@+id/msgAndFrndBtn"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/write_message"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/button_hello"
                    android:text="Сообщение"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp"
                    android:textSize="12sp"
                    android:gravity="center"
                    android:textColor="@color/zalivon" />

                <TextView
                    android:id="@+id/to_friends"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginLeft="1dp"
                    android:background="@drawable/button_hello"
                    android:paddingBottom="10dp"
                    android:paddingTop="10dp"
                    android:textSize="12sp"
                    android:gravity="center"
                    android:textColor="@color/zalivon" />

            </LinearLayout>

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:background="@color/main_white_color_hello"
            android:orientation="vertical">

            <LinearLayout
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:tint="@color/main_dark_grey_color_hello"
                        app:srcCompat="@drawable/ic_home_black_24dp"/>

                    <TextView
                        android:id="@+id/cityCache"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center_vertical"
                        android:text=" "
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/phoneNumProfileConteiner"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:tint="@color/main_dark_grey_color_hello"
                        app:srcCompat="@mipmap/ic_action_call"/>

                    <TextView
                        android:id="@+id/phoneNumProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center_vertical"
                        android:text=" "
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/skypeProfileConteiner"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:tint="@color/main_dark_grey_color_hello"
                        app:srcCompat="@drawable/ic_action_skype"/>

                    <TextView
                        android:id="@+id/skypeProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center_vertical"
                        android:text=" "
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/dohodProfileContener"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:tint="@color/main_dark_grey_color_hello"
                        app:srcCompat="@drawable/ic_action_dohod" />

                    <TextView
                        android:id="@+id/dohodProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/autoProfileContener"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:tint="@color/main_dark_grey_color_hello"
                        app:srcCompat="@drawable/ic_action_auto" />

                    <TextView
                        android:id="@+id/autoProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/rostAndVesProfileContener"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/rostTitleProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical"
                        android:text="Рост"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/rostProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/vesTitleProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical"
                        android:text="Вес"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/vesProfile"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp" />

                </LinearLayout>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/new_view"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/friendsAndGiftsBlock"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:background="@color/main_white_color_hello"
            android:orientation="vertical">

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/friendsCountButton"
                        android:layout_width="116dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/friendsCount"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:layout_marginLeft="10dp"
                            android:gravity="center_vertical"
                            android:text="Нет друзей"/>

                        <LinearLayout
                            android:id="@+id/friendsBlock"
                            android:layout_width="116dp"
                            android:layout_height="wrap_content"
                            android:visibility="gone"
                            android:padding="10dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/friendsLayout1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="3dp"
                                android:visibility="gone"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/friend1"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_marginRight="3dp"
                                    android:visibility="gone" />

                                <ImageView
                                    android:id="@+id/friend2"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_marginRight="3dp"
                                    android:visibility="gone" />

                                <ImageView
                                    android:id="@+id/friend3"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:visibility="gone" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/friendsLayout2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="3dp"
                                android:visibility="gone"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/friend4"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_marginRight="3dp"
                                    android:visibility="gone" />

                                <ImageView
                                    android:id="@+id/friend5"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_marginRight="3dp"
                                    android:visibility="gone" />

                                <ImageView
                                    android:id="@+id/friend6"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:visibility="gone" />

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/groupsCountButton"
                        android:layout_width="116dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/groupsCount"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:layout_marginLeft="10dp"
                            android:gravity="center_vertical"
                            android:text="Нет друзей"/>

                        <LinearLayout
                            android:id="@+id/groupsBlock"
                            android:layout_width="116dp"
                            android:layout_height="wrap_content"
                            android:visibility="gone"
                            android:padding="10dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/groupsLayout1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="3dp"
                                android:visibility="gone"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/groups1"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_marginRight="3dp"
                                    android:visibility="gone" />

                                <ImageView
                                    android:id="@+id/groups2"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_marginRight="3dp"
                                    android:visibility="gone" />

                                <ImageView
                                    android:id="@+id/groups3"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:visibility="gone" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/groupsLayout2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="3dp"
                                android:visibility="gone"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/groups4"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_marginRight="3dp"
                                    android:visibility="gone" />

                                <ImageView
                                    android:id="@+id/groups5"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_marginRight="3dp"
                                    android:visibility="gone" />

                                <ImageView
                                    android:id="@+id/groups6"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:visibility="gone" />

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </HorizontalScrollView>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/new_view"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_marginTop="5dp"
            android:background="@color/main_white_color_hello"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/giftsCount"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_marginLeft="10dp"
                android:gravity="center_vertical"
                android:text="Нет подарков"
                android:textSize="15dp" />

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="6dp"
                android:scrollbars="none">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/giftsLL"
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <GridView
                            android:id="@+id/giftsGrid"
                            android:layout_width="wrap_content"
                            android:layout_height="60dp"
                            android:paddingLeft="10dp"
                            android:layout_marginBottom="10dp"
                            android:columnWidth="60dp"
                            android:gravity="center"
                            android:horizontalSpacing="4dp"
                            android:scrollbarAlwaysDrawHorizontalTrack="true"
                            android:scrollbarAlwaysDrawVerticalTrack="true"
                            android:scrollbars="horizontal"
                            android:stretchMode="none"
                            android:verticalSpacing="1dp"/>

                    </LinearLayout>

                </FrameLayout>

            </HorizontalScrollView>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/new_view"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/lookinForBar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:background="@color/main_white_color_hello"
            android:orientation="vertical">

            <LinearLayout
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_weight="1"
                    android:text="Ищу"
                    android:textColor="@color/main_blue_color_hello"
                    android:textSize="15dp" />

                <View
                    android:id="@+id/findView"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="-2dp"
                    android:layout_marginRight="-2dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/view" />

                <TextView
                    android:id="@+id/looking_for"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_weight="1"
                    android:text=""
                    android:textColor="@color/main_black_color_hello"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/ageLookinFor"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_weight="1"
                    android:text=""
                    android:textColor="@color/main_black_color_hello"
                    android:textSize="15dp" />

                <LinearLayout
                    android:id="@+id/celZnakomstvaProfileLL"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="-2dp"
                        android:layout_marginRight="-2dp"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/view" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Цель знакомства"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile0"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Дружба и общение"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Новые знакомства"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Переписка"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Серьезные отношения"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Брак"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Секс"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Занятия спортом"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Бизнес"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Путешествия"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:text="Свидания и встречи"
                        android:textColor="@color/main_black_color_hello"
                        android:textSize="15dp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/celZnakomstvaProfile10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
